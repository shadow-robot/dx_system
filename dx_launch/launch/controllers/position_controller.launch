<launch>
<arg name="hand_type"/>
<arg name="output" default="screen"/>
<arg name="side"/>

<rosparam command="load" file="$(find dx_config)/controllers/config/$(arg hand_type)/$(arg side)/position_controller.yaml"/>
<node name="spawn_position_controller"
      pkg="controller_manager" type="spawner" respawn="false"
      output="$(arg output)"
      args="position_controller"/>
</launch>
